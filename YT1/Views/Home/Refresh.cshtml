@model YT1.Models.ThongKeView

@if (Model != null)
{
    <div class="statistical">
        <table class="table">
            <tr>
                <td>Hôm Nay</td>
                <td>@Model.HomNay.ToString("N0")</td>
            </tr>
            <tr>
                <td>Hôm Qua</td>
                <td>@Model.HomQua.ToString("N0")</td>
            </tr>
            <tr>
                <td>Tuần Này</td>
                <td>@Model.TuanNay.ToString("N0")</td>
            </tr>
            <tr>
                <td>Online</td>
                <td>@Model.VisitorsOnline.ToString("N0")</td>
            </tr>
        </table>
    </div>
}
